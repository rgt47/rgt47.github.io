issn = {0197-2456},
journal = {Controlled clinical trials},
keywords = {Aged,Binomial Distribution,Computer Simulation,Female,Humans,Male,Medical Oncology,Middle Aged,Prognosis,Random Allocation,Randomized Controlled Trials as Topic,Randomized Controlled Trials as Topic: methods,Rectal Neoplasms,Rectal Neoplasms: epidemiology,Rectal Neoplasms: mortality,Rectal Neoplasms: surgery,Survival Rate},
