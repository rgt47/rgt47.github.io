journal = {Journal of Computational and Graphical Statistics},
keywords = {batch means,hastings,log-linear model,metropolis,reference set,regen-},
number = {4},
pages = {730--745},
title = {{A markov chain Monte Carlo algorithm for approximating exact conditional probabilities}},
