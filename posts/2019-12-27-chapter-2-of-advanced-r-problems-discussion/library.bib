keywords = {Aged,Alzheimer Disease,Alzheimer Disease: pathology,Brain,Brain Mapping,Brain Mapping: methods,Brain: pathology,Cognition Disorders,Cognition Disorders: pathology,Databases, Factual,Disease Progression,Female,Humans,Imaging, Three-Dimensional,Imaging, Three-Dimensional: methods,Longitudinal Studies,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Temporal Lobe,Temporal Lobe: pathology,Time Factors},
