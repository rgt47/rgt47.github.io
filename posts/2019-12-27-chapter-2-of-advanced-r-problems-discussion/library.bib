file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Jim{\'{e}}nez-Gamero et al. - 2017 - Fast tests for the two-sample problem based on the empirical characteristic function.pdf:pdf},
issn = {03784754},
journal = {Mathematics and Computers in Simulation},
keywords = {Characteristic function,Consistency,Two-sample problem,Weighted bootstrap},
pages = {390--410},
title = {{Fast tests for the two-sample problem based on the empirical characteristic function}},
volume = {137},
year = {2017}
}
