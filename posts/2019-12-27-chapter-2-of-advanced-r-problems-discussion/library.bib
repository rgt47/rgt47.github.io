title = {{Computerized assessment of pain drawing area: A pilot study.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2732011{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {5},
year = {2009}
}
