journal = {Journal of biopharmaceutical statistics},
number = {6},
pages = {1104--1114},
pmid = {28402165},
publisher = {Taylor {\&} Francis},
title = {{Statistical properties of continuous composite scales and implications for drug development.}},
url = {https://doi.org/10.1080/10543406.2017.1315819},
volume = {27},
year = {2017}
}
