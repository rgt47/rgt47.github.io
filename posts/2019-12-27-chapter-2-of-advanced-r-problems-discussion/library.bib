title = {{Power calculations for clinical trials in Alzheimer's disease.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3568927{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {26 Suppl 3},
