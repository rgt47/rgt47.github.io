issn = {18758908},
journal = {Journal of Alzheimer's Disease},
keywords = {Florbetapir,[18F]fluorodeoxyglucose positron emission tomograp,mild cognitive impairment,structural magnetic resonance imaging},
number = {4},
pages = {1143--1159},
title = {{Accurate prediction of conversion to Alzheimer's disease using imaging, genetic, and neuropsychological biomarkers}},
