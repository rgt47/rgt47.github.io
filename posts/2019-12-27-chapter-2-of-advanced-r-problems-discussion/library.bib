issn = {1863-2661},
journal = {Brain structure {\&} function},
month = {jul},
pmid = {23832599},
title = {{Impact of a multi-nutrient diet on cognition, brain metabolism, hemodynamics, and plasticity in apoE4 carrier and apoE knockout mice.}},
