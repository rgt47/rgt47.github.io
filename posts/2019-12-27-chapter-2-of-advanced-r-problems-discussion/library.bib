file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Vellas et al. - 2011 - Prevention trials in Alzheimer's disease an EU-US task force report.pdf:pdf},
issn = {1873-5118},
journal = {Progress in neurobiology},
keywords = {Advisory Committees,Alzheimer Disease,Alzheimer Disease: diagnosis,Alzheimer Disease: prevention {\&} control,Biological Markers,Biological Markers: metabolism,Clinical Trials, Phase III as Topic,Clinical Trials, Phase III as Topic: methods,European Union,Humans,International Cooperation,Multicenter Studies as Topic,United States},
