keywords = {Amyotrophic Lateral Sclerosis,Amyotrophic Lateral Sclerosis: drug therapy,Clinical Trials, Phase II as Topic,Database Management Systems,Humans,Internet,Placebos,Placebos: therapeutic use,Randomized Controlled Trials as Topic,Ubiquinone,Ubiquinone: analogs {\&} derivatives,Ubiquinone: therapeutic use,Vitamins,Vitamins: therapeutic use},
