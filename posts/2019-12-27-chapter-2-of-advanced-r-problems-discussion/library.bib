journal = {Lancet neurology},
keywords = {Aged,Aged, 80 and over,Alzheimer Disease,Alzheimer Disease: drug therapy,Alzheimer Disease: metabolism,Alzheimer Disease: pathology,Amyloid beta-Peptides,Amyloid beta-Peptides: metabolism,Antibodies, Monoclonal, Humanized,Antibodies, Monoclonal, Humanized: pharmacology,Antibodies, Monoclonal, Humanized: therapeutic use,Brain,Brain: drug effects,Brain: metabolism,Brain: pathology,Female,Humans,Magnetic Resonance Imaging,Male,Middle Aged,Neuroimaging,Retrospective Studies,Risk Factors},
pages = {241--9},
pmid = {22305802},
title = {{Amyloid-related imaging abnormalities in patients with Alzheimer's disease treated with bapineuzumab: a retrospective analysis.}},
