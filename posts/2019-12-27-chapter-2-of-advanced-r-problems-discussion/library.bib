doi = {10.1001/archinternmed.2011.2287},
file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Pan et al. - 2012 - Red meat consumption and mortality results from 2 prospective cohort studies.pdf:pdf},
