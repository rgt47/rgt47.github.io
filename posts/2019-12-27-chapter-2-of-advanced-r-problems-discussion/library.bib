file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Lautenschlager et al. - 2008 - Effect of physical activity on cognitive function in older adults at risk for Alzheimer disease a randomi.pdf:pdf},
issn = {1538-3598},
