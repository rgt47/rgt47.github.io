isbn = {0000000000000},
issn = {0098-7484},
journal = {Jama},
number = {22},
pages = {2305},
pmid = {28609533},
title = {{Association Between Elevated Brain Amyloid and Subsequent Cognitive Decline Among Cognitively Normal Persons}},
url = {http://jama.jamanetwork.com/article.aspx?doi=10.1001/jama.2017.6669},
volume = {317},
