isbn = {0000281913},
issn = {1526-7598},
journal = {Anesthesia and analgesia},
keywords = {Aged,Analgesia, Patient-Controlled,Analgesia, Patient-Controlled: methods,Analgesics, Opioid,Analgesics, Opioid: administration {\&} dosage,Analgesics, Opioid: adverse effects,Female,Fentanyl,Fentanyl: administration {\&} dosage,Fentanyl: adverse effects,Fever,Fever: chemically induced,Humans,Iontophoresis,Iontophoresis: methods,Male,Middle Aged,Morphine,Morphine: administration {\&} dosage,Morphine: adverse effects,Nausea,Nausea: chemically induced,Pain Measurement,Pain Measurement: methods,Pain, Postoperative,Pain, Postoperative: drug therapy,Randomized Controlled Trials as Topic},
