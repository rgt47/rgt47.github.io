pmid = {19437497},
title = {{Fully automatic hippocampus segmentation and classification in Alzheimer's disease and mild cognitive impairment applied on data from ADNI.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2837195{\&}tool=pmcentrez{\&}rendertype=abstract},
