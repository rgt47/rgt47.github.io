journal = {Neuropsychopharmacology : official publication of the American College of Neuropsychopharmacology},
keywords = {Adult,Aged,Aging,Aging: psychology,Cognition,Cognition: physiology,Computers,Face,Female,Humans,Male,Memory,Memory, Short-Term,Memory, Short-Term: physiology,Memory: physiology,Middle Aged,Neuropsychological Tests,Psychomotor Performance,Psychomotor Performance: drug effects,Psychomotor Performance: physiology,Reference Values,Reproducibility of Results},
month = {nov},
