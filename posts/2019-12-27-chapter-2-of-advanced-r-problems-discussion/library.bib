journal = {Minerva anestesiologica},
keywords = {Analgesics, Opioid,Analgesics, Opioid: administration {\&} dosage,Analgesics, Opioid: pharmacology,Analgesics, Opioid: therapeutic use,Chronic Disease,Clinical Trials as Topic,Delayed-Action Preparations,Humans,Neoplasms,Neoplasms: complications,Oxycodone,Oxycodone: administration {\&} dosage,Oxycodone: pharmacology,Oxycodone: therapeutic use,Pain,Pain: drug therapy,Pain: etiology},
