doi = {10.1002/ana.21509},
file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Amieva et al. - 2008 - Prodromal Alzheimer's disease successive emergence of the clinical symptoms.pdf:pdf},
