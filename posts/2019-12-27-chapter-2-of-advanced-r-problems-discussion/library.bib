journal = {The journal of nutrition, health {\&} aging},
keywords = {abstract,ad,alzheimer,and mild cognitive impairment,clinical trials efficiency,every six,mci,measures,missing data,mixed effects models,mixed model of repeated,mmrm,or functional assessments repeated,quantitative review,randomized clinical trials of,s disease,typically assess intervention efficacy,with measures of cognitive},
number = {4},
pages = {360--364},
