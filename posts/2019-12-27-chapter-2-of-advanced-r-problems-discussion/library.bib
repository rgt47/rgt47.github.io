file = {:Users/zenn/Library/Application Support/Mendeley Desktop/Downloaded/Blennow et al. - 2012 - Effect of immunotherapy with bapineuzumab on cerebrospinal fluid biomarker levels in patients with mild to moder.pdf:pdf},
isbn = {1538-3687 (Electronic)$\backslash$r0003-9942 (Linking)},
