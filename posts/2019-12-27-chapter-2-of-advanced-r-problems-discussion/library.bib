journal = {Alzheimer's and Dementia},
keywords = {Activities of daily living,Alzheimer's Disease Assessment Scale (ADAS),Alzheimer's disease,Amyloid-beta protein,Cholinesterase inhibitors,Clinical Dementia Rating scale,Clinical global impression of change,Clinical trials,Clinical trials methods,Memantine},
